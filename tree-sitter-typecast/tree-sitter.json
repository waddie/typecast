{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "typecast",
      "camelcase": "typecast",
      "title": "typecast",
      "scope": "source.typecast",
      "file-types": [
        "qp"
      ],
      "injection-regex": "^typecast$",
      "class-name": "TreeSitterTypecast"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for typecast .qp scripts",
    "authors": [
      {
        "name": "Tom Waddington",
        "email": "tom@waddington.dev"
      }
    ],
    "links": {
      "repository": "https://github.com/waddie/typecast"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}